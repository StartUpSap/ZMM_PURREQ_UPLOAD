projection;
strict ( 2 );
use draft;

define behavior for ZMM_C_SOLPED_HEADER alias Header
{
  use create;
  use update;
  use delete;

  use action Edit;
  use action Activate;
  use action Discard;
  use action Resume;
  use action Prepare;

  use association _Detail { create; with draft; }

  use action actionCreatePurchaseReq;
}

define behavior for ZMM_c_SOLPED_DETAIL alias Detail
{
  use update;
  use delete;

  use association _Header { with draft; }
}